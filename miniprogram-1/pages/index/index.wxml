<!--index.wxml-->
<view class="container" style="background:#d9f0b4;">
   <view style="width:100%; height:100%;">
      <image src='../images/top.jpg' style="width:100%; height:200px;"></image>
      <button class='btn1' bindtap="go_camera" style="margin-top: 80rpx;">
         <image class='btn1Img' src='../images/camera.png'></image>
         <view>拍照识别</view>
      </button>
	
      <button class='btn2' bindtap="go_photo_album">
         <image class='btn2Img' src='../images/photo_album.png'></image>
         <view>图片识别</view>
      </button>
   </view>
</view>
